+int mdprintf(int desc, const char *str, ...)
+{
+	va_list args;
+	char buff[512];
+	int bytes;
+
+    va_start(args, str);
+    bytes = vsnprintf(buff, sizeof(buff), str, args);
+    va_end(args);
+
